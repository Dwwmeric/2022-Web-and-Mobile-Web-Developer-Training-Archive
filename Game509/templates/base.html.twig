<!DOCTYPE html>
<html lang="fr">
    <head>
        <meta charset="UTF-8">
        <meta http-equive="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta name="HandheldFriendly" content="true">
        <title>{% block title %}TheGame509{% endblock %}</title>

        <meta property="og:url"           content="https://boardgame.studio509.dev/game" />
        <meta property="og:type"          content="website" />
        <meta property="og:title"         content="THE GAME509" />
        <meta property="og:description"   content="Je dÃ©veloper par le studio 509 " />
        <meta property="og:image"         content="asset/image/logoSlide1.png" />   
            
        {% block stylesheets %}
            <link rel="stylesheet" type="text/css" href="{{ asset ('asset/css/style.css') }}">
            <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
            <link href="https://fonts.googleapis.com/css2?family=Bungee+Inline&display=swap" rel="stylesheet">
            <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
            <link rel="stylesheet" href="https://unpkg.com/swiper/swiper-bundle.min.css"/>
            <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@400;700&display=swap" rel="stylesheet">
            <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css" rel="stylesheet">
        {% endblock %}

        {% block javascripts %}
            <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
            <script src="https://code.jquery.com/jquery-3.6.0.min.js" integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>
            <script type="text/javascript" src="{{ asset ('asset/js/ajax.js') }}"></script>   
            <script type="text/javascript" src="{{ asset ('asset/js/script.js') }}"></script>
             <script src="https://cdnjs.cloudflare.com/ajax/libs/html2canvas/1.4.0/html2canvas.min.js"></script>
        {% endblock %}
    </head>
    <body class="body" onload="init()">
        {% block body %}{% endblock %}

          {% if menu_to_open is defined  %}
        <script>
            var menu_to_open = "{{ menu_to_open }}";   
        </script>
            {% endif %}  



      
        <script type="text/javascript" src="{{ asset ('asset/js/main.js') }}"></script>
        <script src="https://unpkg.com/swiper/swiper-bundle.min.js"></script>
        <script>
        /* Initialize Swiper */

  var swiper_horizontal_1 = new Swiper(".mySwiperHorizontal1", {
    spaceBetween: 0,
    allowTouchMove:false,
    touchMoveStopPropagation:true,
     navigation: {
        nextEl: '.mySwiperHorizontal1 .nextH',
        prevEl: '.mySwiperHorizontal1 .prevH',
    },
  });
  
  var swiper_horizontal_2 = new Swiper(".mySwiperHorizontal2", {
    spaceBetween: 0,
    allowTouchMove:false,
    touchMoveStopPropagation:true,
     navigation: {
        nextEl: '.mySwiperHorizontal2 .nextH',
        prevEl: '.mySwiperHorizontal2 .prevH',
    },
  });

  var swiper_vertical = new Swiper(".mySwiperVertical", {
    direction: "vertical",
    touchMoveStopPropagation:true,
    allowTouchMove:false,
    spaceBetween: 0,
     navigation: {
        nextEl: '.mySwiperVertical .nextV',
        prevEl: '.mySwiperVertical .prevV',
    },
  });  

   

        </script>
    </body>

</html>
